{
  "parser": "babel-eslint",
  "extends": "atomikui",
  "settings": {
    "import/resolver": {
      "alias": {
        "map": [
          ["@atomikui-core", "./node_modules/@atomikui/core/dist/components"]
        ]
      }
    }
  },
  "rules": {
    "no-underscore-dangle": 0,
    "react/forbid-prop-types": 0,
    "no-console": 0
  }
}
